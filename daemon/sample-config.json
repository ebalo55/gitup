{
  "$schema": "https://raw.githubusercontent.com/ebalo55/gitup/refs/heads/master/daemon/config-schema.json",
  "operational_mode": "full",
  "folder_branching": false,
  "split": true,
  "split_size": 25,
  "split_unit": "megabytes",
  "multipart": true,
  "paths": [
    "./daemon/src/main.rs",
    "./gui/"
  ],
  "debug": true,
  "compress": true,
  "encrypt": true,
  "key": "password",
  "providers": [
    "gitup://<auth-token>:<provider-name>/<provider-dependant-fragments>"
  ]
}